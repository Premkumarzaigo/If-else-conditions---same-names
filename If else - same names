<!DOCTYPE html>
<html>
<body>

<h2>JavaScript if .. else</h2>

<p id="demo"></p>

<script>
const time = new Date().getHours();
let greeting;
if (time < 10) {
  greeting = "apple";
} else if (time < 10) {
  greeting = "apple";
} else {
  greeting = "Pass";
}
document.getElementById("demo").innerHTML = greeting;
</script>

</body>
</html>
